<ResourceDictionary 
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:patchs="using:Typedown.XamlUI.Patchs"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Style TargetType="ComboBox" x:Key="DefaultComboBoxStyle" BasedOn="{StaticResource DefaultComboBoxStyle}">
        <Setter Property="patchs:ComboBoxPatch.Apply" Value="True"/>
    </Style>

    <Style TargetType="ComboBox" BasedOn="{StaticResource DefaultComboBoxStyle}"/>

</ResourceDictionary>