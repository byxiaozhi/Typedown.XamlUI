<ResourceDictionary 
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:patchs="using:Typedown.XamlUI.Patchs"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Style x:Key="DefaultContentDialogStyle" TargetType="ContentDialog" BasedOn="{StaticResource DefaultContentDialogStyle}">
        <Setter Property="patchs:ContentDialogPatch.Apply" Value="True"/>
    </Style>

    <Style TargetType="ContentDialog" BasedOn="{StaticResource DefaultContentDialogStyle}"/>

</ResourceDictionary>